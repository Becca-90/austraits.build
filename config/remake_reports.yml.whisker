targets:
{{#study_names}}
  export/reports/{{study_name}}.md:
    knitr:
      input: reports/report.Rmd
      chdir: false
      auto_figure_prefix: true
    depends:
      - data_study: {{study_name}}
      - austraits

    cleanup_level: purge
  export/reports/{{study_name}}.html:
    command: render_html("export/reports/{{study_name}}.md")
    cleanup_level: purge
{{/study_names}}
  reports:
    depends:
{{#study_names}}
      - export/reports/{{study_name}}.html
{{/study_names}}
